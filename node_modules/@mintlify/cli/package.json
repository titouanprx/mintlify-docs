{
  "name": "@mintlify/cli",
  "version": "4.0.322",
  "description": "The Mintlify CLI",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "cli"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": "./bin/index.js",
  "bin": {
    "mintlify": "bin/index.js"
  },
  "scripts": {
    "dev": "yarn build && node bin/index.js dev",
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "clean:build": "rimraf bin",
    "clean:all": "rimraf node_modules .eslintcache && yarn clean:build",
    "watch": "tsc --watch",
    "lint": "eslint src --cache",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "@mintlify/common": "1.0.219",
    "@mintlify/link-rot": "3.0.312",
    "@mintlify/models": "0.0.158",
    "@mintlify/prebuild": "1.0.311",
    "@mintlify/previewing": "4.0.316",
    "@mintlify/validation": "0.1.245",
    "chalk": "^5.2.0",
    "detect-port": "^1.5.1",
    "js-yaml": "^4.1.0",
    "ora": "^6.1.2",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@mintlify/eslint-config-typescript": "1.0.13",
    "@mintlify/prettier-config": "1.0.4",
    "@mintlify/ts-config": "2.0.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@tsconfig/recommended": "1.x",
    "@types/detect-port": "^1.3.2",
    "@types/node": "^18.7.13",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/parser": "6.x",
    "eslint": "8.x",
    "openapi-types": "^12.0.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.5.3",
    "vitest": "^2.0.4"
  },
  "gitHead": "45f6023c970a8d4e5880f19ca631d26b8119d89b"
}
